{
    "extends": "airbnb-base/legacy",
    "rules": {
      "object-curly-spacing": 0,
      "func-names": 0,
      "no-shadow": 0,
      "vars-on-top": 0,
      "no-param-reassign": 0,
      "consistent-return": 0,
      "no-extend-native": 0,
      "no-plusplus": 0,
      "no-use-before-define": [2, { "functions": false, "variables": false }]
    }
}
